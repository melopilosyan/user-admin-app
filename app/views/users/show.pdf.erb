
<div class="user-details-container">
  <div class="logo">
    <%= wicked_pdf_image_tag @user.logo.url(:thumb) %>
  </div>
  <dl class="dl-horizontal">
    <dt>Full name:</dt>
    <dd><%= @user.full_name %></dd>

    <dt>Email:</dt>
    <dd><%= @user.email %></dd>

    <dt>Birthday:</dt>
    <dd><%= @user.birthday %></dd>

    <dt>BIO:</dt>
    <dd><%= @user.bio %></dd>
  </dl><br>
</div>